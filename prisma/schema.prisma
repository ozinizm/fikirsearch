generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Lead {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  name      String
  sector    String
  city      String
  country   String
  address   String?
  phone     String?
  website   String?
  rating    Float?
  placeId   String   @unique
  lat       Float?
  lng       Float?
}
